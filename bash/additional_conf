export EDITOR="vim"

# some more ls aliases
alias ll='ls -l'
alias la='ls -A'
alias l='ls -CF'

function cs () {
	cd $1
	ls
}
 
source ~/.git-completion.bash

# git branch prompt
export GITAWAREPROMPT=~/.bash/git-aware-prompt
source $GITAWAREPROMPT/main.sh
export PS1="\u@\h:\w\[$txtgrn\]\$git_branch\[$txtred\]\$git_dirty\[$txtrst\]\$ "
